{
  "name": "react-transform-boilerplate",
  "version": "1.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build-client": "node_modules/.bin/rollup -c rollup.client.config.js",
    "build-server": "node_modules/.bin/rollup -c rollup.server.config.js",
    "build-all": "npm run build-client & npm run build-server",
    "build-and-run": "npm run build-all & node devServer.bundle.js",
    "watch": "node_modules/.bin/nodemon --watch src --exec \"npm run build-client & npm run build-server & node devServer.bundle.js\"",
    "start": "node devServer.js",
    "debug": "node-debug devServer.js",
    "lint": "eslint src",
    "test": "mocha  --compilers js:babel-core/register --recursive --reporter spec src",
    "test-cpu": "mocha -w --compilers js:babel-core/register test/gbc/CPUTest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/gaearon/react-transform-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-transform-boilerplate",
  "devDependencies": {
    "chai": "*",
    "eslint": "^3.3.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^6.1.2",
    "express": "^4.13.4",
    "mocha": "*",
    "modular-css": "^0.27.0",
    "nodemon": "^1.10.2",
    "rimraf": "^2.4.3",
    "rollup": "^0.34.10",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-includepaths": "^0.1.5",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-root-import": "^0.2.0"
  },
  "dependencies": {
    "redux": "^3.5.2",
    "ws": "^1.1.1"
  }
}
